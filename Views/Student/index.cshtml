@{
    ViewBag.Title = "Students";
}
      
            <header class="jumbotron">
    <h1>Hello from index.cshtml</h1>
            </header>
        <table class="table table-hover">
             
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Age</th>
                    </tr>
                </thead>
                <tbody>
                @foreach(var stud in @Model)
                {
                <tr>
                    <td>@stud.FirstName</td>
                    <td> @stud.LastName</td> 
                    <td>@stud.Age</td>
                </tr>
                }
                </tbody>
        </table>